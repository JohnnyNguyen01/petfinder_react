Loading = () => dispatch({ type: SET_LOADING });\n\n  return (\n    <AuthContext.Provider\n      value={{\n        user: state.user,\n        error: state.error,\n        loading: state.loading,\n        setAuthError,\n        setLoading,\n        signUp,\n        setUser,\n        loginUser,\n      }}\n    >\n      {props.children}\n    </AuthContext.Provider>\n  );\n};\n\nexport default AuthState;\n","/Users/johnnynguyen/React Projects/petfinder_react/src/contexts/auth/AuthReducer.js",[],"/Users/johnnynguyen/React Projects/petfinder_react/src/contexts/types.js",[],"/Users/johnnynguyen/React Projects/petfinder_react/src/firebase.js",[],"/Users/johnnynguyen/React Projects/petfinder_react/src/pages/home/Home.js",[],"/Users/johnnynguyen/React Projects/petfinder_react/src/pages/signup/SignUp.js",["57","58"],"import React, { useRef, useEffect, useState, useContext } from \"react\";\nimport { Form, Button, Card, Alert, Container } from \"react-bootstrap\";\nimport AuthContext from \"../../contexts/auth/authContext\";\nimport {Link} from \"react-router-dom\";\n\nconst SignUp = () => {\n  const emailRef = useRef();\n  const passwordRef = useRef();\n  const passwordConfirmRef = useRef();\n  const authContext = useContext(AuthContext);\n\n  const handleSubmit = async (e) => {\n    const emailText = emailRef.current.value;\n    e.preventDefault();\n    if (passwordRef.current.value !== passwordConfirmRef.current.value) {\n      authContext.setAuthError(\"Passwords do not match\");\n    } else if (emailText === null || emailText === \"\"){\n      authContext.setAuthError(\"Please enter a valid email address\")\n    }else\n      try {\n        authContext.setLoading();\n        await authContext.signUp(\n          emailRef.current.value,\n          passwordRef.current.value\n        );\n      } catch (e) {\n        authContext.setAuthError(e);\n      }\n  };\n\n  return (\n    <Container\n      className=\"d-flex align-items-center justify-content-center\"\n      style={{ minHeight: \"100vh\" }}\n    >\n      <div className=\"w-100\" style={{ maxWidth: \"400px\" }}>\n        <Card>\n          <Card.Body>\n            {authContext.error && (\n              <Alert variant=\"danger\">{authContext.error}</Alert>\n            )}\n            <h2 className=\"text-center mb-4\">Sign Up</h2>\n            <Form>\n              <Form.Group id=\"email\">\n                <Form.Label>Email</Form.Label>\n                <Form.Control type=\"email\" ref={emailRef} required />\n              </Form.Group>\n              <Form.Group id=\"password\">\n                <Form.Label>Password</Form.Label>\n                <Form.Control type=\"password\" ref={passwordRef} required />\n              </Form.Group>\n              <Form.Group id=\"password-confirm\">\n                <Form.Label>Password Confirmation</Form.Label>\n                <Form.Control\n                  type=\"password\"\n                  ref={passwordConfirmRef}\n                  required\n                />\n              </Form.Group>\n              <Button\n                disabled={authContext.loading}\n                className=\"w-100\"\n                type=\"submit\"\n                onClick={handleSubmit}\n              >\n                Sign Up\n              </Button>\n            </Form>\n          </Card.Body>\n        </Card>\n\n        <div className=\"w-100 text-center mt-2\">\n          Already have an account? <Link to=\"/login\">Log In</Link>\n        </div>\n      </div>\n    </Container>\n  );\n};\n\nexport default SignUp;\n","/Users/johnnynguyen/React Projects/petfinder_react/src/pages/login/Login.js",[],"/Users/johnnynguyen/React Projects/petfinder_react/src/components/PrivateRoute.js",[],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":3,"column":10,"nodeType":"65","messageId":"66","endLine":3,"endColumn":19},{"ruleId":"63","severity":1,"message":"67","line":1,"column":8,"nodeType":"65","messag